{
  "name": "@starknet-dapp/monorepo",
  "version": "1.0.0",
  "keywords": [
    "ethereum",
    "react",
    "starknet"
  ],
  "private": true,
  "scripts": {
    "react-app:build": "yarn workspace @starknet-dapp/react-app build --max-old-space-size=12288",
    "react-app:eject": "yarn workspace @starknet-dapp/react-app eject",
    "react-app:start": "yarn workspace @starknet-dapp/react-app start",
    "react-app:test": "yarn workspace @starknet-dapp/react-app test",
    "build": "yarn workspace @starknet-dapp/react-app build --max-old-space-size=12288",
    "prettier": "yarn workspace @starknet-dapp/react-app prettier",
    "chain": "yarn workspace @starknet-dapp/hardhat chain",
    "fork": "yarn workspace @starknet-dapp/hardhat fork",
    "node": "yarn workspace @starknet-dapp/hardhat chain",
    "test": "yarn workspace @starknet-dapp/hardhat test",
    "start": "yarn workspace @starknet-dapp/react-app start",
    "compile": "yarn workspace @starknet-dapp/hardhat compile",
    "deploy": "yarn workspace @starknet-dapp/hardhat deploy",
    "verify": "yarn workspace @starknet-dapp/hardhat verify",
    "watch": "yarn workspace @starknet-dapp/hardhat watch",
    "accounts": "yarn workspace @starknet-dapp/hardhat accounts",
    "balance": "yarn workspace @starknet-dapp/hardhat balance",
    "send": "yarn workspace @starknet-dapp/hardhat send",
    "ipfs": "yarn workspace @starknet-dapp/react-app ipfs",
    "surge": "yarn workspace @starknet-dapp/react-app surge",
    "s3": "yarn workspace @starknet-dapp/react-app s3",
    "ship": "yarn workspace @starknet-dapp/react-app ship",
    "generate": "yarn workspace @starknet-dapp/hardhat generate",
    "deployNewAccount": "yarn workspace @starknet-dapp/hardhat deployNewAccount",
    "fundedwallet": "cd packages/hardhat && npx hardhat fundedwallet",
    "flatten": "cd packages/hardhat && npx hardhat flatten",
    "clean": "cd packages/hardhat && npx hardhat clean",
    "theme": "yarn workspace @starknet-dapp/react-app theme",
    "watch-theme": "yarn workspace @starknet-dapp/react-app watch",
    "postinstall": "husky install"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/hardhat",
      "**/hardhat/**",
      "**/hardhat-ts",
      "**/hardhat-ts/**"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^7.0.2"
  }
}
